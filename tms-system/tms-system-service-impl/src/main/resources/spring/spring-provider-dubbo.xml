    <dubbo:service interface="com.kaishengit.tms.system.service.AccountService" ref="accountService"/>
    <dubbo:service interface="com.kaishengit.tms.system.service.ScenicService" ref="scenicService"/>
    <dubbo:service interface="com.kaishengit.tms.system.service.ScenicAccountService" ref="scenicAccountService"/>-->

</beans>